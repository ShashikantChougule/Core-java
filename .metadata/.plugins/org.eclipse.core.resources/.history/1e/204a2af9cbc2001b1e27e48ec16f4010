package com.collection;

import java.util.ArrayList;
import java.util.List;

public class UseList {
	
	public static void main(String[] args) {
		
		List list=new ArrayList();
		list.add(100);
		list.add(123);
		list.add("Hello");
		list.add(true);
		
		for(Object obj:list) {
			if(obj instanceof Integer) {
				Integer ival=(Integer) obj;
			}
		}
	}

}
