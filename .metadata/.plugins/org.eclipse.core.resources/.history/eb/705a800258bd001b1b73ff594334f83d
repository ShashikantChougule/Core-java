package com.ct.training;

public class Dateformat {
	
	int dd;
	String mm;
	int yyyy;
	
	/*public void init() {
		dd=1;
		mm="may";
		yyyy=2021;
	}*/
	public Dateformat() {
		dd=1;
		mm="may";
		yyyy=2021;
		
	}
	public Dateformat(int d,String m,int y) {
		dd=d;
		mm=m;
		yyyy=y;
		
	}
	public Dateformat(int dd,String mm,int yy) {
		this.dd=dd;
		this.mm=mm;
		this.yyyy=yy;
		
	}
	
public void dispalyDate() {
	System.out.println("Date is: "+dd+"_"+mm+"_"+yyyy);
}



public void setDate(int d,String m,int y) {
         dd=d;
         mm=m;
         yyyy=y;
}
}
