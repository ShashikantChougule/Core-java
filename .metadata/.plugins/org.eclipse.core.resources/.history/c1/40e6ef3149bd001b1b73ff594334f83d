		empId=1;
		empName="Shashi";
		empAddress="Sangli";
	}
public void dispalyDate() {
	System.out.println("/n Employee Details :"+"empId"+"/n empname"+"/n empAddress");
}

public void setDate(int d,String m,int y) {
         dd=d;
         mm=m;
