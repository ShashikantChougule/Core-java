	String empAddress;
	public void init() {
		empId=1;
		empName="Shashi";
		empAddress="Snagli";
	}
public void dispalyDate() {
	System.out.println("Date is: "+dd+"_"+mm+"_"+yyyy);
}

public void setDate(int d,String m,int y) {
         dd=d;
         mm=m;
         yyyy=y;
}
}
